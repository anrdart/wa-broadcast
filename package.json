{
  "name": "whatsapp-web.js",
  "version": "1.31.0",
  "description": "Library for interacting with the WhatsApp Web API ",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "test": "mocha tests --recursive --timeout 5000",
    "test-single": "mocha",
    "shell": "node --experimental-repl-await ./shell.js",
    "generate-docs": "npx jsdoc --configure .jsdoc.json --verbose",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "broadcast": "node server.js",
    "prod": "NODE_ENV=production node server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop wa-broadcast",
    "pm2:restart": "pm2 restart wa-broadcast",
    "pm2:logs": "pm2 logs wa-broadcast",
    "pm2:monit": "pm2 monit",
    "setup:prod": "bash setup-production.sh",
    "docker:build": "docker build -t wa-broadcast .",
    "docker:run": "docker run -d --name wa-broadcast -p 3000:3000 -v $(pwd)/sessions:/app/sessions -v $(pwd)/logs:/app/logs --security-opt seccomp:unconfined --cap-add SYS_ADMIN wa-broadcast",
    "docker:stop": "docker stop wa-broadcast && docker rm wa-broadcast",
    "docker:logs": "docker logs -f wa-broadcast",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:compose:build": "docker-compose build --no-cache",
    "docker:rebuild": "docker-rebuild.bat",
    "docker:rebuild:ps1": "powershell -ExecutionPolicy Bypass -File docker-rebuild.ps1",
    "docker:rebuild:bash": "bash docker-rebuild.sh",
    "docker:health": "curl -f http://localhost:3000/health || echo 'Health check failed'",
    "docker:health:win": "powershell -Command \"try { Invoke-RestMethod -Uri http://localhost:3000/health } catch { Write-Host 'Health check failed' }\"",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "audit:fix": "npm audit fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedroslopez/whatsapp-web.js.git"
  },
  "keywords": [
    "whatsapp",
    "whatsapp-web",
    "api",
    "bot",
    "client",
    "node"
  ],
  "author": "Pedro Lopez",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pedroslopez/whatsapp-web.js/issues"
  },
  "homepage": "https://wwebjs.dev/",
  "dependencies": {
    "@pedroslopez/moduleraid": "^5.0.2",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "fluent-ffmpeg": "2.1.3",
    "mime": "^4.0.7",
    "node-fetch": "^2.6.9",
    "node-webpmux": "3.1.7",
    "puppeteer": "^24.13.0",
    "qrcode": "^1.5.4",
    "whatsapp-web.js": "^1.31.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.12",
    "chai": "^4.3.4",
    "chai-as-promised": "^8.0.1",
    "eslint": "^8.4.1",
    "eslint-plugin-mocha": "^10.0.3",
    "jsdoc": "^3.6.4",
    "jsdoc-baseline": "^0.1.5",
    "mocha": "^11.7.1",
    "nodemon": "^3.0.1",
    "sinon": "^13.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "optionalDependencies": {
    "archiver": "^5.3.1",
    "fs-extra": "^10.1.0",
    "unzipper": "^0.10.11"
  }
}
